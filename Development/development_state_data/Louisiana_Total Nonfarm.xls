<script>

function tls_version() {
  var tls = headerObj["x-ssl-protocol"];
  
 if (tls === "tlsv1.0" || tls ==="tlsv1"){
	var tlsValue = bls_getCookie("TLS_Version");	
	if (tlsValue === null || tlsValue === undefined || tlsValue === "") {
		bls_setCookie("TLS_Version",tls,365);
		$("body").prepend('<a id="tlsMessageTrigger" rel="#tlsMessage" style="display:none;"></a>');
		$("body").prepend('<div class="bls-chartdata-overlay" id="tlsMessage" style="max-width: 562px;"><div class="bls-overlay-heading"><a href="javascript:void(0);">close</a> or Esc Key</div><a class="close"></a><p style="padding: 15px; font-size:19px;"><img src="/images/icons/icon_lrg_special_notice.gif" style="vertical-align:top; display:inline-block; width:9%; left: -4px; position:relative;" /><strong style="display:inline-block; width:89%">Security Updates: New requirements to access BLS websites starting October 1, 2018</strong></p><p style="padding:0 15px 15px 15px; font-size: 14px; line-height: 1.6em;">Starting October 1st, you will no longer be able to access BLS websites using your current browser. BLS is updating its website security. To access our websites, you will need to have a browser with compatible security features. For more information, see: <a href="/bls/disable-transport-layer-security-1-0.htm">https://www.bls.gov/bls/disable-transport-layer-security-1-0.htm</a>.</p></div>');
		$("#tlsMessageTrigger[rel]").overlay({
			top:200,
			left:"center",
			onLoad:function(e){
				$("#tlsMessage .bls-overlay-heading a").click(function(){
					$("#tlsMessageTrigger[rel]").overlay().close();
				});
				$("#tlsMessage").css("left",(($(window).width()/2) - ($("#tlsMessage").width()/2)) + "px");
			},
			closeOnClick:false,
			mask: {
			color: '#000',
			loadSpeed: 200,
			opacity: 0.5,
			zIndex: 10000
			}
		});
		  $("#tlsMessageTrigger").click();

		}
	}
}


  var headerObj = {};
  (function getHeader(){
	var req = new XMLHttpRequest();
	if(window.XDomainRequest){
		req.open('GET', document.location);
		req.onreadystatechange = function(){
			if(req.readyState != 4){return false;}
			if(req.status != 200){return false;}
			headersToObj(req.getAllResponseHeaders());
			return true;
		}
	  }else{
		req.open('HEAD', document.location, true);
		req.onload = function(e){
			if(req.readyState === 4)  {
			  if(req.status === 200) {
				  headersToObj(req.getAllResponseHeaders());
				}
			  }
		};  
	}
	req.send(null);
  })();

  function headersToObj(headerStr){
	var headerStr = String(headerStr).toLowerCase().split("\r\n");
	for(index in headerStr){
		headerArr = headerStr[index].split(": ");
		if(headerArr[1]){headerObj[headerArr[0]] = headerArr[1];}
	}
  }

  Object.size = function(obj) {
	var size = 0, key;
	for (key in obj) {
		if (obj.hasOwnProperty(key)) size++;
	}
	return size;  
  };

  var headerInterval = setInterval(function(){
	if(Object.size(headerObj)){
		clearInterval(headerInterval);
		tls_version();
	}
  },500);


</script>
	
	
